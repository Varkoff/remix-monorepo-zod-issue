{
  "name": "@virgile/frontend",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "imports": {
    "components/*": ["./app/components/*"],
    "types/*": ["./app/types/*"],
    "server/*": ["app/server/*"],
    "schemas/*": ["./app/schemas/*"],
    "utils/*": ["./app/utils/*"],
    "routes/*": ["./app/routes/*"],
    "hooks/*": ["./app/hooks/*"],
    "public/*": ["./public/*"],
    "styles/*": ["./app/styles/*"]
  },
  "scripts": {
    "dev": "remix vite:dev",
    "build": "remix vite:build",
    "start": "remix-serve ./build/server/index.js",
    "lint": "npx @biomejs/biome lint ./app --apply",
    "typecheck": "tsc",
    "format": "npx @biomejs/biome format ./ --write"
  },
  "dependencies": {
    "@conform-to/react": "^1.1.2",
    "@conform-to/zod": "^1.1.2",
    "@virgile/shared": "*",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/express": "^2.9.1",
    "@remix-run/node": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "@remix-run/serve": "^2.9.1",
    "@tw-classed/react": "^1.7.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "is-ip": "^5.0.1",
    "isbot": "^4.1.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "react-leaflet": "^4.2.1",
    "remix-flat-routes": "^0.6.4",
    "spin-delay": "^1.2.0",
    "tailwind-merge": "^2.3.0",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.23.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.1",
    "@remix-run/dev": "^2.9.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.19",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-cjs-interop": "^2.1.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.1",
    "vitest-mock-extended": "^1.3.1"
  },
  "engines": {
    "node": "20"
  }
}
